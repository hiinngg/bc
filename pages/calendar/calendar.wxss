/* pages/calendar/calendar.wxss */
.notice{
background:#FFF9DF;
color: #CFC392;
font-size:14px;
line-height: 2.6;
text-align: center;
}

.day view,.date view{
  width: 14.2857143%;
  text-align: center;
  font-size:14px;
  line-height: 2.6;
  padding:6rpx 6rpx;
  box-sizing:border-box;
 
 
}
.day{
  height: 78rpx;
  background: #596e8d;
  color: #ffffff ;
  font-size:14px;
}
.date_disable{
   display: block;
  color: #cacaca;
}

.date_today{
   display: block;
  background: #cacaca !important;
  color:#ffffff;
  border-radius: 3px;
}
.date_active{
  display: block;
  background: #5f9df1 !important;
  color:#ffffff;
  border-radius: 3px;

}
.ym{
  line-height: 3;
  font-weight: bold;
  font-size:14px;

}